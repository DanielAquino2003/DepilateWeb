{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios';\nexport default {\n  __name: 'App',\n  setup(__props) {\n    const email = ref('');\n    const name = ref('');\n    const registered = ref(false);\n    const isLoading = ref(false);\n    const error = ref('');\n    const handleSubmit = async () => {\n      if (isLoading.value) return;\n      isLoading.value = true;\n      error.value = '';\n      try {\n        const response = await axios.post('http://localhost:5000/api/register', {\n          name: name.value,\n          email: email.value\n        }, {\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        });\n        console.log('Respuesta del servidor:', response.data);\n        registered.value = true;\n      } catch (err) {\n        console.error('Error al registrar:', err);\n        error.value = 'Hubo un error al registrarse. Por favor, intenta de nuevo.';\n      } finally {\n        isLoading.value = false;\n      }\n    };\n    return {\n      __sfc: true,\n      email,\n      name,\n      registered,\n      isLoading,\n      error,\n      handleSubmit\n    };\n  }\n};","map":{"version":3,"names":["ref","axios","email","name","registered","isLoading","error","handleSubmit","value","response","post","headers","console","log","data","err"],"sources":["App.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <header>\n      <h1 class=\"main-title\">LISTENING PARTY</h1>\n    </header>\n    <div class=\"registration-container\">\n      <div v-if=\"!registered\">\n        <h2>Registro para la Fiesta</h2>\n        <form @submit.prevent=\"handleSubmit\">\n          <div class=\"form-group\">\n            <label for=\"email\">Email:</label>\n            <input type=\"email\" id=\"email\" v-model=\"email\" required>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"name\">Nombre:</label>\n            <input type=\"text\" id=\"name\" v-model=\"name\" required>\n          </div>\n          <button type=\"submit\" :disabled=\"isLoading\">\n            {{ isLoading ? 'Registrando...' : 'Registrarse' }}\n          </button>\n        </form>\n        <p v-if=\"error\" class=\"error-message\">{{ error }}</p>\n      </div>\n      <div v-else>\n        <h2>BIENVENIDO A LA LISTENING PARTY EN VILLA SKORRITZE</h2>\n        <p class=\"registered-name\">ยก{{ name }}, no olvides venir depilado!</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from 'vue'\nimport axios from 'axios'\n\nconst email = ref('')\nconst name = ref('')\nconst registered = ref(false)\nconst isLoading = ref(false)\nconst error = ref('')\n\nconst handleSubmit = async () => {\n  if (isLoading.value) return\n\n  isLoading.value = true\n  error.value = ''\n\n  try {\n    const response = await axios.post('http://localhost:5000/api/register', {\n      name: name.value,\n      email: email.value\n    }, {\n      headers: {\n        'Content-Type': 'application/json'\n      }\n    })\n\n    console.log('Respuesta del servidor:', response.data)\n    registered.value = true\n  } catch (err) {\n    console.error('Error al registrar:', err)\n    error.value = 'Hubo un error al registrarse. Por favor, intenta de nuevo.'\n  } finally {\n    isLoading.value = false\n  }\n}\n</script>\n\n<style>\nbody {\n  margin: 0;\n  padding: 0;\n  font-family: Arial, sans-serif;\n}\n\n.app-container {\n  display: flex;\n  flex-direction: column;\n  min-height: 100vh;\n  background: linear-gradient(135deg, rgba(0, 0, 0, 0.8) 0%, rgba(255, 165, 0, 0.8) 100%);\n}\n\nheader {\n  padding: 2rem 0;\n  text-align: center;\n}\n\n.main-title {\n  font-size: 3rem;\n  color: #FFA500;\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);\n  margin: 0;\n}\n\n.registration-container {\n  background-color: rgba(0, 0, 0, 0.7);\n  padding: 2rem;\n  border-radius: 8px;\n  width: 100%;\n  max-width: 400px;\n  color: #FFA500;\n  margin: auto;\n}\n\nh2 {\n  text-align: center;\n  margin-bottom: 2rem;\n}\n\n.form-group {\n  margin-bottom: 1rem;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 0.5rem;\n}\n\ninput {\n  width: 100%;\n  padding: 0.5rem;\n  background-color: rgba(0, 0, 0, 0.5);\n  border: 1px solid #FFA500;\n  color: #FFA500;\n  border-radius: 4px;\n}\n\nbutton {\n  width: 100%;\n  padding: 0.5rem;\n  background-color: #FFA500;\n  color: black;\n  border: none;\n  cursor: pointer;\n  border-radius: 4px;\n  transition: background-color 0.3s ease;\n}\n\nbutton:hover:not(:disabled) {\n  background-color: #FF8C00;\n}\n\nbutton:disabled {\n  opacity: 0.7;\n  cursor: not-allowed;\n}\n\n.error-message {\n  color: #FF4500;\n  text-align: center;\n  margin-top: 1rem;\n}\n</style>"],"mappings":"AAgCA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,KAAK,MAAM,OAAO;;;;IAEzB,MAAMC,KAAK,GAAGF,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMG,IAAI,GAAGH,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMI,UAAU,GAAGJ,GAAG,CAAC,KAAK,CAAC;IAC7B,MAAMK,SAAS,GAAGL,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMM,KAAK,GAAGN,GAAG,CAAC,EAAE,CAAC;IAErB,MAAMO,YAAY,GAAG,MAAAA,CAAA,KAAY;MAC/B,IAAIF,SAAS,CAACG,KAAK,EAAE;MAErBH,SAAS,CAACG,KAAK,GAAG,IAAI;MACtBF,KAAK,CAACE,KAAK,GAAG,EAAE;MAEhB,IAAI;QACF,MAAMC,QAAQ,GAAG,MAAMR,KAAK,CAACS,IAAI,CAAC,oCAAoC,EAAE;UACtEP,IAAI,EAAEA,IAAI,CAACK,KAAK;UAChBN,KAAK,EAAEA,KAAK,CAACM;QACf,CAAC,EAAE;UACDG,OAAO,EAAE;YACP,cAAc,EAAE;UAClB;QACF,CAAC,CAAC;QAEFC,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAEJ,QAAQ,CAACK,IAAI,CAAC;QACrDV,UAAU,CAACI,KAAK,GAAG,IAAI;MACzB,CAAC,CAAC,OAAOO,GAAG,EAAE;QACZH,OAAO,CAACN,KAAK,CAAC,qBAAqB,EAAES,GAAG,CAAC;QACzCT,KAAK,CAACE,KAAK,GAAG,4DAA4D;MAC5E,CAAC,SAAS;QACRH,SAAS,CAACG,KAAK,GAAG,KAAK;MACzB;IACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}